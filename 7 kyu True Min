7 kyu True Min
https://www.codewars.com/kata/52378b3ee72f21e1ea000045

const min = (a, b) => { 
  if (isNaN(a) === true || isNaN(b) === true) return NaN;
  else if (a === undefined || b === undefined) return NaN;
  else if (a === null)  a = 0;
  else if (b === null)  b = 0;
  else if (a === -Infinity || b === -Infinity) return -Infinity
  const arr = [a, b];
  arr.sort();
  return arr[0]
}
