/*
*   7 kyu Find array
*   https://www.codewars.com/kata/59a2a3ba5eb5d4e609000055
* */

const findArray = (arr1, arr2) => {
  if (arr1.length == 0 || arr2.length == 0) return [];
  let x = []
  let result = arr2.map(ele => {
    if (arr1.length > ele) x.push(arr1[ele])
  });
  console.log(result);
  return x;
}
